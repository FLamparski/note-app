{
  "name": "note-app",
  "version": "1.0.0",
  "description": "Interesting notes",
  "main": "index.js",
  "scripts": {
    "lint": "xo --fix",
    "test": "xo --fix && cross-env NODE_ENV=test mocha --compilers js:babel-core/register",
    "serve-dev": "cross-env NODE_ENV=development webpack-dev-server --inline --hot --progress",
    "build": "cross-env NODE_ENV=production webpack -p"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FLamparski/note-app.git"
  },
  "keywords": [
    "notes"
  ],
  "author": "Filip Wieland",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/FLamparski/note-app/issues"
  },
  "homepage": "https://github.com/FLamparski/note-app#readme",
  "devDependencies": {
    "autoprefixer": "^7.1.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-webpack-alias": "^2.1.2",
    "babel-preset-env": "^1.5.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "cross-env": "^5.0.0",
    "css-loader": "^0.28.3",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-import-resolver-webpack": "^0.8.1",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-react": "^7.0.1",
    "expect.js": "^0.3.1",
    "extract-text-webpack-plugin": "^2.1.0",
    "html-webpack-plugin": "^2.28.0",
    "less": "^2.7.2",
    "less-loader": "^4.0.3",
    "mocha": "^3.4.2",
    "node-sass": "^4.5.3",
    "postcss-loader": "^2.0.5",
    "sass-loader": "^6.0.5",
    "style-loader": "^0.18.1",
    "svg-sprite-loader": "^3.0.2",
    "webpack": "^2.6.1",
    "webpack-dev-middleware": "^1.10.2",
    "webpack-dev-server": "^2.4.5",
    "worker-loader": "^0.8.0",
    "xo": "^0.18.2"
  },
  "dependencies": {
    "lodash": "^4.17.4",
    "material-design-icons": "^3.0.1",
    "moment": "^2.18.1",
    "preact": "^8.1.0",
    "preact-compat": "^3.16.0",
    "preact-redux": "^2.0.1",
    "react-toolbox": "^2.0.0-beta.12",
    "redux": "^3.6.0",
    "redux-saga": "^0.15.3",
    "regenerator-runtime": "^0.10.5",
    "shortid": "^2.2.8"
  },
  "xo": {
    "envs": [
      "browser"
    ],
    "settings": {
      "import/resolver": "webpack"
    },
    "plugins": [
      "eslint-plugin-react",
      "eslint-plugin-import",
      "eslint-plugin-jsx-a11y",
      "eslint-plugin-jsx-a11y"
    ],
    "rules": {
      "react/jsx-uses-vars": 2
    }
  }
}
